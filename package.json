    "autoprefixer": "^10.4.14",
    "dayjs": "^1.11.9",
    "fs-extra": "^11.1.1",
    "postcss": "^8.4.24",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.45.0",
    "react-redux": "^8.1.2",
    "tailwindcss": "^3.3.3",
    "yup": "^1.2.0",
    "next": "13.5.6"
  }
}